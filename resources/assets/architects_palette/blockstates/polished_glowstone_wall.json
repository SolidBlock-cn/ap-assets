{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "architects_palette:block/fancy_walls/polished_glowstone/post"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "architects_palette:block/fancy_walls/polished_glowstone/side_low_front"
      }
    },
    {
      "when": {
        "east": "low"
      },
